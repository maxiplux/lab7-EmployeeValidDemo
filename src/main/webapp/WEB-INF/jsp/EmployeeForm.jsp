<%@ page language="java" contentType="text/html; charset=utf-8" pageEncoding="utf-8"%><%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><!DOCTYPE HTML><html><head>    <title><spring:message code="addEmployeeForm"/></title>    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.3/moment.min.js"></script><style type="text/css">@import url("<c:url value="/css/main.css"/>");</style></head><body><div id="global">    <spring:message code="language"/> : <a href="?language=en_US">English</a>|<a href="?language=es_CO">Espanol</a>    <form:form modelAttribute="employee" action="employee_save" method="post" enctype="multipart/form-data">        <fieldset>            <legend><spring:message code="addAnEmployee"/></legend>            <p>                <form:errors path="*" cssStyle="color : red;" />            </p>            <p>                <label for="id"><spring:message code="id"/> </label>                    <form:input path="id" id="id" />            <div style="text-align: center;">                <form:errors path="id" cssStyle="color : red;" />            </div>            </p>            <p>                <label for="firstName"><spring:message code="firstName"/></label>                    <form:input path="firstName" />            <div style="text-align: center;">                <form:errors path="firstName" cssStyle="color : red;" />            </div>            </p>            <p>                <label><spring:message code="lastName"/></label>                    <form:input path="lastName" />            <div style="text-align: center;">                <form:errors path="lastName" cssStyle="color : red;" />            </div>            </p>            <p>                <label for="birthDate"><spring:message code="dateOfBirth"/></label>                <form:input path="birthDate" type="text"   value="10-14-1983"  id="birthDate" />                <form:errors path="birthDate" cssStyle="color : red;" />            </p>            <p>                <label for="salary"><spring:message code="salaryLevel"/></label>                    <form:input path="salaryLevel" id="salary" />            <div style="text-align: center;">                <form:errors path="salaryLevel" cssStyle="color : red;" />            </div>            </p>            <h4><spring:message code="address"/></h4>            <p>                <label for="street"><spring:message code="address.street"/></label>                    <form:input path="address.street" id="street" />            <div style="text-align: center;">                <form:errors path="address.street" cssStyle="color : red;" />            </div>            </p>            <p>                <label for="state"><spring:message code="state"/> </label>                    <form:input path="address.state" id="state" />            <div style="text-align: center;">                <form:errors path="address.state" cssStyle="color : red; " />            </div>            </p>            <p>                <label for="zipCode"><spring:message code="zipCode"/> </label>                    <form:input path="address.zipCode" id="zipCode" />            <div style="text-align: center;">                <form:errors path="address.zipCode" cssStyle="color : red; " />            </div>            </p>            <p>                <label><spring:message code="photo"/></label>                <form:input path="photo" id="photo" type="file"/>            </p>            <p id="buttons">                <input id="submit" type="submit"  value="<spring:message code="add"/>">            </p>        </fieldset>    </form:form></div></body></html>